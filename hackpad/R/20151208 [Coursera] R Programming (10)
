$(document).on("click", "a[target]", function(e) {
  e.preventDefault();
  var newWindow = window.open("", this.target || "_blank");
  if (newWindow) {
    newWindow.opener = null;
    newWindow.location = this.href;
    newWindow.focus();
  }
});
